{
	"name": "The Importer by Integromat",
	"version": "1.0.2",
	"description": "The Importer by Integromat allows you to import apps to Integromat in a few clicks.",
	"manifest_version": 2,
	"background": {
		"scripts": [
			"background.mjs"
		],
		"presistent": false
	},
	"browser_action": {
		"default_popup": "index.html"
	},
	"permissions": [
		"declarativeContent",
		"activeTab",
		"storage",
		"tabs",
		"cookies",
		"*://*.zapier.com/*",
		"*://*.integromat.com/*",
		"*://*/*"
	],
	"icons": {
		"16": "icon/16.png",
		"32": "icon/32.png",
		"48": "icon/48.png",
		"128": "icon/128.png",
		"1024": "icon/1024.png"
	}
}